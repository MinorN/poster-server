  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true,
    "require": []
  },
  "scripts": {
    "start": "eggctl start --daemon --title=egg-server-poster-server",
    "stop": "eggctl stop --title=egg-server-poster-server",
    "dev": "egg-bin dev",
    "test:local": "egg-bin test",
    "pretest": "npm run clean && npm run lint -- --fix",
    "test": "egg-bin test",
    "preci": "npm run clean && npm run lint",
    "ci": "egg-bin cov",
    "postci": "npm run prepublishOnly && npm start && npm stop && npm run clean",
    "lint": "eslint . --ext .ts --cache",
    "tsc": "tsc",
    "clean": "tsc -b --clean",
    "prepublishOnly": "npm run clean && npm run tsc",
    "build:template:dev": "npx webpack --config webpack/webpack.config.js",
    "build:template:prod": "npx webpack --config webpack/webpack.config.js --env production && npm run upload",
    "upload": "node webpack/uploadToOSS.js"
  },
